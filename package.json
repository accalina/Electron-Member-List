{
  "name": "hunterlist",
  "version": "1.0.0",
  "description": "A Test Desktop App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Member List\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/github.com/accalina.git"
  },
  "keywords": [
    "Test",
    "Desktop",
    "App",
    "Lists"
  ],
  "author": "Accalina Shirosachi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/github.com/accalina/issues"
  },
  "homepage": "https://github.com/github.com/accalina#readme",
  "dependencies": {
    "electron": "^2.0.5"
  },
  "devDependencies": {
    "electron-packager": "^12.1.0"
  }
}
